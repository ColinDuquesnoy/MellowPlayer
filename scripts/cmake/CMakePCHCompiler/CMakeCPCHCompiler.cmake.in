# Copy compiler information from C compiler.

set(CMAKE_CPCH_COMPILER "@CMAKE_C_COMPILER@")
set(CMAKE_CPCH_COMPILER_ARG1 "@CMAKE_C_COMPILER_ARG1@")
set(CMAKE_CPCH_COMPILER_ID "@CMAKE_C_COMPILER_ID@")
set(CMAKE_CPCH_COMPILER_VERSION "@CMAKE_C_COMPILER_VERSION@")
set(CMAKE_CPCH_PLATFORM_ID "@CMAKE_C_PLATFORM_ID@")
set(CMAKE_CPCH_SIMULATE_ID "@CMAKE_C_SIMULATE_ID@")
set(CMAKE_CPCH_SIMULATE_VERSION "@CMAKE_C_SIMULATE_VERSION@")
@SET_MSVC_CPCH_ARCHITECTURE_ID@
set(CMAKE_CPCH_COMPILER_LOADED 1)
set(CMAKE_CPCH_COMPILER_WORKS @CMAKE_C_COMPILER_WORKS@)
set(CMAKE_CPCH_ABI_COMPILED @CMAKE_C_ABI_COMPILED@)

set(CMAKE_CPCH_COMPILER_ENV_VAR "CPCH")

set(CMAKE_CPCH_COMPILER_ID_RUN 1)

# Save compiler ABI information.
set(CMAKE_CPCH_SIZEOF_DATA_PTR "@CMAKE_C_SIZEOF_DATA_PTR@")
set(CMAKE_CPCH_COMPILER_ABI "@CMAKE_C_COMPILER_ABI@")
set(CMAKE_CPCH_LIBRARY_ARCHITECTURE "@CMAKE_C_LIBRARY_ARCHITECTURE@")

@CMAKE_CPCH_SYSROOT_FLAG_CODE@
@CMAKE_CPCH_OSX_DEPLOYMENT_TARGET_FLAG_CODE@
